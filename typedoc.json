{
    "$schema": "https://typedoc.org/schema.json",
    "name": "MediaSFU Angular",
    "entryPoints": ["src/docs.ts"],
    "entryPointStrategy": "expand",
    "groupOrder": [ "Components", "Hooks", "Types", "Utils" ],
    "kindSortOrder": ["Class","Function", "Interface", "TypeAlias", "Variable"],
    "packageOptions": {
      "entryPoints": ["src/docs.ts"],
      "groupOrder": [ "Components", "Hooks", "Types", "Utils" ],
      "kindSortOrder": ["Function", "Interface", "TypeAlias"],
      "readme": "none"
    },
    "out": "docs",
    "sidebarLinks": {
      "MediaSFU": "https://mediasfu.com",
    },
    "highlightLanguages": [
        "bash",
        "console",
        "css",
        "html",
        "javascript",
        "json",
        "jsonc",
        "json5",
        "tsx",
        "typescript",
      "jsx"
    ],
    "theme": "default",
    "plugin": ["typedoc-plugin-extras"],
    "logLevel": "Error",
    "hostedBaseUrl": "https://mediasfu.com/angular",
    "useHostedBaseUrlForAbsoluteLinks": false,
    "customFooterHtml": "&copy; 2024 <strong>MediaSFU</strong>",
    "navigation": {
        "includeCategories": true,
        "includeGroups": true,
        "includeFolders": true
    },
    "categorizeByGroup": true,
    "hideGenerator":true,
    "sort": ["source-order", "kind", "alphabetical"],
    "customTitle": "MediaSFU Angular",
    "customDescription" : "MediaSFU Angular SDK",
    "favicon": "assets/images/favicon.ico"
  }
